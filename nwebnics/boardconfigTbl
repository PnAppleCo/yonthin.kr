CREATE TABLE IF NOT EXISTS wboardConfig (
  idx int(10) unsigned NOT NULL AUTO_INCREMENT,
  code varchar(20) CHARACTER SET utf8 NOT NULL DEFAULT '',
  board_summary varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  list_level tinyint(2) unsigned DEFAULT '10',
  write_level tinyint(2) unsigned DEFAULT '10',
  reply_level tinyint(2) unsigned DEFAULT '10',
  edit_level tinyint(2) unsigned DEFAULT '10',
  delete_level tinyint(2) unsigned DEFAULT '10',
  view_level tinyint(2) unsigned DEFAULT '10',
  down_level tinyint(2) unsigned DEFAULT '10',
  board_head text CHARACTER SET utf8,
  board_tail text CHARACTER SET utf8,
  log_info tinyint(1) unsigned DEFAULT '1',
  out_login tinyint(1) unsigned NOT NULL DEFAULT '0',
  num_per_page tinyint(2) unsigned DEFAULT '10',
  num_per_block tinyint(2) unsigned DEFAULT '10',
  reply_indent tinyint(2) unsigned DEFAULT '5',
  adult tinyint(1) unsigned DEFAULT '0',
  noreplebtn tinyint(1) unsigned NOT NULL DEFAULT '0',
  skin varchar(255) CHARACTER SET utf8 DEFAULT 'default',
  title_bar_text varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  meta_keyword varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  menu_depth varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  doctype varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  charset varchar(50) CHARACTER SET utf8 NOT NULL DEFAULT '',
  language varchar(50) CHARACTER SET utf8 NOT NULL DEFAULT '',
  hotclick mediumint(3) unsigned DEFAULT '100',
  allow_delete_thread tinyint(1) unsigned DEFAULT '0',
  overview tinyint(1) unsigned DEFAULT '0',
  twinread tinyint(1) unsigned DEFAULT '0',
  approve tinyint(1) unsigned NOT NULL DEFAULT '0',
  pds_view tinyint(1) unsigned NOT NULL DEFAULT '0',
  subject_cut smallint(6) NOT NULL DEFAULT '50',
  only_img tinyint(1) unsigned DEFAULT '0',
  thumbnail_width smallint(3) unsigned NOT NULL DEFAULT '80',
  thumbnail_height smallint(3) unsigned NOT NULL DEFAULT '60',
  upload_max_size int(10) unsigned DEFAULT '5000000',
  upload_count tinyint(2) unsigned NOT NULL DEFAULT '1',
  img_max_upload_width mediumint(3) NOT NULL DEFAULT '0',
  img_view_size mediumint(4) NOT NULL DEFAULT '800',
  relation_text tinyint(1) unsigned DEFAULT '0',
  write_comment tinyint(1) unsigned DEFAULT '0',
  comment_limit smallint(4) unsigned NOT NULL DEFAULT '200',
  board_class varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  set_cookie_data tinyint(1) unsigned DEFAULT '0',
  recommend tinyint(1) unsigned DEFAULT '0',
  view_layer varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  adminid varchar(12) CHARACTER SET utf8 DEFAULT NULL,
  adminpass varchar(12) CHARACTER SET utf8 DEFAULT NULL,
  best_img tinyint(1) unsigned NOT NULL DEFAULT '0',
  html_editor tinyint(1) unsigned NOT NULL DEFAULT '0',
  ps_center tinyint(1) unsigned NOT NULL DEFAULT '0',
  real_name tinyint(1) unsigned NOT NULL DEFAULT '0',
  onlySecret tinyint(1) unsigned NOT NULL DEFAULT '0',
  top_inc varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  left_inc varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  right_inc varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  bottom_inc varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  atop_inc varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  aleft_inc varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  aright_inc varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  abottom_inc varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  signdate date NOT NULL DEFAULT '0000-00-00',
  PRIMARY KEY (idx),
  KEY code (code)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;