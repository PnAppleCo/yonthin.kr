CREATE TABLE IF NOT EXISTS wBoard (
  idx int(10) unsigned NOT NULL AUTO_INCREMENT,
  code varchar(12) CHARACTER SET utf8 NOT NULL DEFAULT '',
  fid int(10) unsigned NOT NULL DEFAULT '0',
  name varchar(50) CHARACTER SET utf8 NOT NULL DEFAULT '',
  email varchar(40) CHARACTER SET utf8 DEFAULT NULL,
  homepage varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  subject varchar(100) CHARACTER SET utf8 NOT NULL DEFAULT '',
  ucontents text CHARACTER SET utf8 NOT NULL,
  passwd varchar(41) CHARACTER SET utf8 NOT NULL DEFAULT '',
  signdate date NOT NULL DEFAULT '0000-00-00',
  signtime time NOT NULL DEFAULT '00:00:00',
  keytag varchar(255) CHARACTER SET utf8 NOT NULL COMMENT '키태그',
  click mediumint(5) unsigned NOT NULL DEFAULT '0',
  thread varchar(255) CHARACTER SET utf8 NOT NULL DEFAULT '',
  html tinyint(1) NOT NULL DEFAULT '1',
  auto_enter tinyint(1) unsigned NOT NULL DEFAULT '0',
  secret tinyint(1) NOT NULL DEFAULT '0',
  re_email tinyint(1) NOT NULL DEFAULT '0',
  notice tinyint(1) NOT NULL DEFAULT '0',
  approve tinyint(3) unsigned NOT NULL DEFAULT '0',
  filename0 varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  filename1 varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  filename2 varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  filename3 varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  filename4 varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  filename5 varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  filename6 varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  filename7 varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  filename8 varchar(255) CHARACTER SET utf8 DEFAULT NULL,
  patition tinyint(2) NOT NULL DEFAULT '0',
  b_class varchar(50) CHARACTER SET utf8 NOT NULL DEFAULT '',
  mem_id varchar(12) CHARACTER SET utf8 DEFAULT NULL,
  svc_name varchar(50) CHARACTER SET utf8 NOT NULL DEFAULT '',
  svc_status tinyint(1) unsigned NOT NULL DEFAULT '0',
  svc_reply text CHARACTER SET utf8 NOT NULL,
  svc_grade tinyint(1) unsigned NOT NULL DEFAULT '0',
  svc_tel varchar(18) CHARACTER SET utf8 NOT NULL DEFAULT '',
  svc_date datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  recommend smallint(5) unsigned NOT NULL DEFAULT '0',
  etc01 varchar(255) CHARACTER SET utf8 NOT NULL,
  etc02 varchar(255) CHARACTER SET utf8 NOT NULL,
  etc03 varchar(255) CHARACTER SET utf8 NOT NULL,
  userip varchar(20) CHARACTER SET utf8 NOT NULL DEFAULT '',
  PRIMARY KEY (idx),
  KEY code (code)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;